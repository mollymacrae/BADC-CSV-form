{
  "name": "badc-csv-form",
  "version": "1.0.0",
  "description": "Simple client-side form to make a BADC CSV form.",
  "main": "dist/badc-csv-form.min.js",
  "scripts": {
    "build": "esbuild src/index.js --bundle --minify --outfile=dist/badc-csv-form.min.js --global-name=FormToCSV",
    "dev": "esbuild src/index.js --bundle --outfile=dist/badc-csv-form.js --global-name=FormToCSV --watch"
  },
  "devDependencies": {
    "esbuild": "^0.21.0"
  }
}
